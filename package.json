{
	"name": "project",
	"version": "1.0.0",
	"description": "Welcome to the course project repository.",
	"main": "index.js",
	"scripts": {
		"build": "npm run build:client && npm run build:server && npm run build:chat",
		"build:chat": "npm run clean:chat && cd src/client/chat-app-ia && npm i && webpack && cd -",
		"build:client": "node webpack.build.js",
		"build:client:dev": "node webpack.build.js",
		"build:server": "echo SERVER_BUILD=true node webpack.build.js use target: node instead polyfills",
		"check:docker": "docker ps -q > /dev/null 2>&1 && echo 'Docker is running' || echo 'Docker is not running' && exit 1",
		"clear": "rm -rf build build-react package-lock.json",
		"clean:deep": "rm -rf node_modules && rm -rf src/client/chat-app-ia/node_modules && rm -rf src/client/chat-app-ia/dist",
		"clean:chat": "rm -rf src/client/chat-app-ia/dist",
		"docker-postgresql-logs": "docker logs postgres",
		"docker-postgresql-remove": "docker rmi $(docker images -q)",
		"docker-postgresql-rm": "docker rm $(docker ps -a -q)",
		"docker-postgresql-start": "docker start postgres",
		"docker-postgresql-stop": "docker stop $(docker ps -a -q)",
		"docker-postgresql-up": "docker run --name postgres -e POSTGRES_PASSWORD=localpassword -p 5432:5432 -d postgres",
		"lint:chat": "eslint . --ext js  --report-unused-disable-directives",
		"lint": "eslint .",
		"prestart:server": "npm run docker-postgresql-start",
		"start:chat": "npm run build:chat && cd src/client/chat-app-ia && webpack serve --no-client-overlay --server-type https && cd -",
		"start:client": "node webpack.serve.js",
		"start:server": "node src/server/server.js",
		"start": "concurrently \"node docker-service-controller.js\" \"npm run start:client\"  \"npm run start:server\" \"npm run start:chat\"",
		"test:client:wip": "node webpack.test.js",
		"test": "jest",
		"test:server": "echo wip.."
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser"
	},
	"keywords": [
		"nodejs",
		"express",
		"rest",
		"api",
		"sequelize",
		"postgresql"
	],
