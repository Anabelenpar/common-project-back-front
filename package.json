{
  "name": "project",
  "version": "1.0.0",
  "description": "Welcome to the course project repository.",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:server": "node src/server/server.js",
    "prestart:server": "npm run docker-postgresql-start",
    "build:server": "echo wip..",
    "test:server": "echo wip..",
    "docker-postgresql-up": "docker run --name postgres -e POSTGRES_PASSWORD=localpassword -p 5432:5432 -d postgres",
    "docker-postgresql-stop": "docker stop $(docker ps -a -q)",
    "docker-postgresql-remove": "docker rmi $(docker images -q)",
    "docker-postgresql-rm": "docker rm $(docker ps -a -q)",
    "docker-postgresql-logs": "docker logs postgres",
    "docker-postgresql-start": "docker start postgres",
    "start:client": "node scripts/start.js",
    "build:client": "node scripts/build.js",
    "test:client": "node scripts/test.js",
    "eject:client": "node scripts/eject.js",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "postgresql"
  ],
  "author": "jbuckland-veridas",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "@types/connect-livereload": "^0.6.0",
    "@types/express": "^4.17.15",
    "@types/glob": "^8.0.1",
    "@types/livereload": "^0.9.2",
    "@types/node": "^18.11.18",
    "@types/sqlite3": "^3.1.8",
    "axios": "^0.27.2",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "bootstrap": "^4.6.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "connect-livereload": "^0.6.1",
    "cors": "^2.8.5",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv-expand": "^5.1.0",
